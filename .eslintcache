okChars,\n        saveCurrentPok\n    } = props;\n\n    const handleSubmit = event => {\n        event.preventDefault();\n\n        getPokDataByName(pokemonName)\n        .then(pokemonData => {\n            getPokCharById(pokemonData.id)\n            .then(result => saveCurrentPok(pokemonName, pokemonData, result));\n\n            setPokemonData(pokemonData);\n        })\n        .catch(() => alert(\"This pokemon isn't exist\"))\n    }\n\n    const handleChange = event => {\n        setPokemonName(event.target.value)\n    }\n\n    return(\n            <div className='main-container__home-container'>\n                <p\n                    children='PokeSearcher'\n                    className='search__search-phrase' \n                />\n                <form \n                    onSubmit={handleSubmit}\n                    className='header__search'\n                >\n                    <input \n                        // value={pokemonName}\n                        onChange={handleChange}\n                        className='search__input'\n                    />\n                    <input \n                        src={pokeBall}\n                        type='image'\n                        className='search__button'\n                    />\n                </form>\n                {isSubmit && <PokemonWindow/>}\n            </div>   \n    )\n}\n\nexport default React.memo(Home);","/home/zaur/Desktop/HW/pockemon/src/modules/home/index.js",[],"/home/zaur/Desktop/HW/pockemon/src/modules/home/actions.js",[],"/home/zaur/Desktop/HW/pockemon/src/modules/home/selectors.js",[],"/home/zaur/Desktop/HW/pockemon/src/modules/pokemonWindow/index.js",[],"/home/zaur/Desktop/HW/pockemon/src/modules/pokemonWindow/PokemonWindow.jsx",["146","147"],"import React, { useState } from 'react';\nimport './pokemonWindowStyle.css'\n\nconst PokemonWindow = props => {\n    const { \n        pokemonData,\n        closeWindow,\n        pokemonChars\n    } = props;\n\n    const handleClick =